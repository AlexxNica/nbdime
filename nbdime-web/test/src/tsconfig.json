{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["dom", "es5", "es2015.promise", "es2015.collection"],
    "outDir": "../build",
    "noImplicitAny": true,
    "noEmitOnError": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  }
}